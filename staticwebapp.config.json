{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/nhs-calc*"
      },
      {
        "route": "/blog/nhs-pension-should-i-opt-out*"
      },
      {
        "route": "/*",
        "redirect": "/404",
        "statusCode": 404
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      },
      "404": {
        "redirect": "/404",
        "statusCode": 404
      }
    },
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }